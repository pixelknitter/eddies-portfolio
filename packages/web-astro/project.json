{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "web-astro",
  "sourceRoot": "packages/web-astro/src",
  "projectType": "application",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/web-astro/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/packages/web-astro"],
      "options": {
        "jestConfig": "packages/web-astro/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "cd packages/web-astro && astro build"
      },
      "configurations": {
        "production": {}
      }
    },
    "serve": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "cd packages/web-astro && astro dev"
      }
    }
  },
  "tags": ["scope:web-astro"]
}
